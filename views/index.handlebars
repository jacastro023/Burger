<div class="jumbotron jumbotron-fluid backimg">
  <div class="container">
    <h1>Burgers!</h1>
  </div>
</div>

<div class="container">
  <div class="row">
    <div class="col">
      <h2>Burgers that are ready to eat!</h2>

      <ul>
        {{#each burgers}}
        {{#unless eaten}}
        {{> burgers/burger-block eaten=true}}
        {{/unless}}
        {{/each}}
      </ul>
    </div>
    <div class="col">

      <h2>Burgers eaten!</h2>

      <ul>
        {{#each burgers}}
        {{#if eaten}}
        {{> burgers/burger-block eaten=false}}
        {{/if}}
        {{/each}}
      </ul>
    </div>
  </div>
</div>

<div class="container" id="one">
  <div><img src="https://www.hloom.com/images/72-layers.svg" alt="cashier image"></div>
  <p>$5 ea,Cash Only!</p>
</div>
<div class="container" id="one">
  <form class="create-form">

    <div class="form-group">
      <label for="ca">Order Burger:</label>
      <input type="text" id="ca" name="name">
    </div>

    <button type="submit">Add Burger</button>
  </form>
</div>